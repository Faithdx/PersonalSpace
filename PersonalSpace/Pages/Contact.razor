@page "/contact"

<PageTitle>Contact</PageTitle>

<h1>Contact</h1>

<p role="status">Current count: @_currentCount</p>

<MButton Color="primary" OnClick="IncrementCount">Click me</MButton>

@code {
    private int _currentCount = 0;

    private void IncrementCount()
    {
        _currentCount++;
    }
}
